		"react-native": "0.79.5",
		"react-native-safe-area-context": "5.4.0",
		"react-native-web": "0.20.0",
		"react-router-native": "^6.30.0",
		"yup": "^1.7.0"
	},
	"devDependencies": {
		"@babel/core": "^7.20.0",
		"@babel/eslint-parser": "^7.28.4",
		"eslint": "^9.35.0",
		"eslint-plugin-jest": "^29.0.1",
		"eslint-plugin-react": "^7.37.5",
		"eslint-plugin-react-native": "^5.0.0",
		"jest": "^29.7.0",
		"jest-expo": "^54.0.10"
	},
	"private": true
}
